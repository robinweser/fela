{
  "name": "jest-react-fela",
  "version": "12.2.0",
  "description": "Jest helpers for react-fela",
  "typings": "index.d.ts",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "sideEffects": false,
  "files": [
    "LICENSE",
    "README.md",
    "index.d.ts",
    "lib/**",
    "es/**"
  ],
  "scripts": {
    "setup": "pnpm build",
    "clean": "rimraf lib es coverage",
    "build": "pnpm clean && pnpm build:es && pnpm build:lib",
    "build:es": "babel src --out-dir es --ignore **/__tests__",
    "build:lib": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --ignore **/__tests__",
    "build:umd": "echo 1",
    "watch": "npm run watch:lib & npm run watch:es",
    "watch:lib": "BABEL_ENV=commonjs babel src -d lib -w",
    "watch:es": "babel src -d es -w",
    "test": "jest --config=jest.config.js",
    "coverage": "pnpm test --coverage"
  },
  "repository": "https://github.com/robinweser/fela/",
  "keywords": [
    "fela",
    "react-fela",
    "testing",
    "react",
    "jest",
    "jest-utils"
  ],
  "author": "Robin Weser",
  "license": "MIT",
  "peerDependencies": {
    "fela": "*",
    "react": "*",
    "react-dom": "*",
    "react-fela": "*"
  },
  "dependencies": {
    "jest-fela-bindings": "^12.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.13.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.5.5",
    "babel-core": "7.0.0-bridge.0",
    "cross-env": "^6.0.3",
    "fela": "^12.2.0",
    "fela-preset-web": "^12.2.0",
    "jest": "^26.6.0",
    "react": "^17.0.2",
    "react-18": "npm:react@^18.1.0",
    "react-dom": "^17.0.2",
    "react-dom-18": "npm:react-dom@^18.1.0",
    "react-fela": "^12.2.0",
    "rimraf": "^3.0.0"
  }
}
